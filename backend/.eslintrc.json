{
  "env": {
    "browser": true,
    "es6": true
  },
  "extends": ["prettier", "eslint:recommended", "plugin:react/recommended"],
  "globals": {
    "Atomics": "readonly",
    "SharedArrayBuffer": "readonly"
  },
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true,
      "js": true
    },
    "ecmaVersion": 2018,
    "sourceType": "module"
  },
  "plugins": ["react"],
  "rules": {
    "prettier/prettier": "error",
    "camelcase": "warn",
    "react/jsx-filename-extension": ["on", { "extensions": [".js", ".jsx"] }]
  }
}
